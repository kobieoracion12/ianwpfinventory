<Window x:Class="NavigationDrawerPopUpMenu2.windows.win_receipt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NavigationDrawerPopUpMenu2.windows"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        xmlns:rv="clr-namespace:Microsoft.Reporting.WinForms;assembly=Microsoft.ReportViewer.WinForms"
        Title="win_receipt" Height="600" Width="400" MaxWidth="500">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <WindowsFormsHost Margin="0 0 0 0" Width="300" Height="600">
            <rv:ReportViewer x:Name="ReportViewerDemo" />
        </WindowsFormsHost>
    </Grid>
</Window>
