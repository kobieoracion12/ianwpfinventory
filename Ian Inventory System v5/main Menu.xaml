<Window x:Class="NavigationDrawerPopUpMenu2.main_Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NavigationDrawerPopUpMenu2"
        mc:Ignorable="d"
        Title="main_Menu" Height="600" Width="1080">

    <Grid>
        <Grid x:Name="GridMenu" Width="70" HorizontalAlignment="Left" Background="#ffffff">
            <Grid.Effect>
                <DropShadowEffect BlurRadius="2" ShadowDepth="1" Color="#FFAEAEAE"/>
            </Grid.Effect>
        </Grid>
    </Grid>
</Window>
